{
  "expo": {
    "name": "Controle de Portaria",
    "slug": "app-portaria",
    "scheme": "app-portaria",
    "version": "3.0.0",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#29ABE2"
    },
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          },
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": "https://n"
      },
      "eas": {
        "projectId": "d15a14e3-012a-4bf0-96c7-adb9d6d22e32"
      }
    },
    "android": {
      "package": "com.andersonbrodrigues.appportaria"
    }
  }
}
